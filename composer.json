{
    "name": "ezadtv/two",
    "description": "Experimental v2 framework",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/asset": "^4.3",
        "symfony/cache": "^4.3",
        "symfony/console": "^4.3",
        "symfony/filesystem": "^4.3",
        "symfony/process": "^4.3",
        "vlucas/phpdotenv": "^3.6",
        "symfony/event-dispatcher": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/inflector": "^4.3",
        "symfony/options-resolver": "^4.3",
        "twig/twig": "^2.0",
        "nikic/fast-route": "^1.3",
        "monolog/monolog": "^2.0",
        "nikic/php-parser": "^4.2",
        "symfony/http-kernel": "^4.3",
        "respect/validation": "^1.1",
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
